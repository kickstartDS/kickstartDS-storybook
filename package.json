{
  "name": "@kickstartds/design-system",
  "version": "2.4.4",
  "author": "ruhmesmeile GmbH <mail@ruhmesmeile.com> (https://ruhmesmeile.com)",
  "homepage": "https://ruhmesmeile.com",
  "license": "SEE LICENSE IN LICENSE",
  "main": "dist/index.js",
  "types": "dist/components/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "init-tokens": "kickstartDS tokens init --cleanup --rc-only",
    "build-tokens": "kickstartDS tokens compile --cleanup --rc-only",
    "build-storybook": "yarn build-tokens && run-p sass sass-manager schema token && DESIGN_TOKEN_GLOB=.storybook/tokens.css build-storybook --output-dir storybook",
    "chromatic": "chromatic --project-token $CHROMATIC_PROJECT_TOKEN --storybook-build-dir 'storybook' --skip '@(renovate/**|dependabot/**)' --exit-zero-on-changes --exit-once-uploaded",
    "dist:js": "esbuild index=src/dist.js --outdir=dist --format=esm --bundle --minify --splitting",
    "dist:sass": "sass --load-path=node_modules src/dist.scss dist/index.css",
    "dist:components": "node scripts/bundleComponents.js",
    "dist:schema": "yarn schema && node scripts/copySchema.js && yarn typings",
    "dist:images": "cp -r static/img static/patterns static/logo.svg static/logo-dark.svg dist/",
    "dist:favicons": "cp -r static/favicon dist/",
    "prepublishOnly": "rm -rf dist && mkdir -p dist && run-p 'build-tokens' 'schema' 'token' && run-p dist:*",
    "sass": "sass --load-path=node_modules src/index.scss static/index.css",
    "sass-manager": "sass --load-path=node_modules .storybook/manager.scss static/manager.css",
    "token": "node scripts/customPropertyExtract.js",
    "typings": "tsc",
    "watch:schema": "yarn schema && chokidar \"src/**/*.schema.json\" -c \"yarn schema {path}\"",
    "watch:token": "yarn build-tokens && yarn token && chokidar \"src/**/*-tokens.scss\" -c \"yarn token {path}\"",
    "watch:sass": "yarn sass --watch",
    "schema": "node scripts/schema.js",
    "start": "yarn sass && run-p watch:* storybook",
    "storybook": "DESIGN_TOKEN_GLOB=.storybook/tokens.css start-storybook -p 6006"
  },
  "keywords": [],
  "dependencies": {
    "@bedrock-layout/primitives": "^2.2.1",
    "@kickstartds/base": "2.0.0-canary.922.4419.0",
    "@kickstartds/blog": "2.0.0-canary.922.4419.0",
    "@kickstartds/content": "2.0.0-canary.922.4419.0",
    "@kickstartds/core": "2.0.0-canary.922.4419.0",
    "@kickstartds/form": "2.0.0-canary.922.4419.0",
    "change-case": "^4.1.2",
    "classnames": "^2.3.1",
    "json-schema-to-typescript": "^10.1.5",
    "json-schema-traverse": "^1.0.0",
    "postinstall-postinstall": "^2.1.0",
    "styled-components": "^5.3.6",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@auto-it/npm": "^10.29.3",
    "@auto-it/released": "^10.29.3",
    "@babel/core": "^7.14.3",
    "@kickstartds/storybook-addon-component-tokens": "^0.1.6",
    "@kickstartds/storybook-addon-jsonschema": "^1.0.5",
    "@storybook/addon-a11y": "^6.5.13",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/theming": "^6.5.9",
    "@whitespace/storybook-addon-html": "git+https://github.com/lmestel/storybook-addon-html.git#v5.3.0",
    "auto": "^10.29.3",
    "babel-loader": "^8.2.2",
    "chokidar-cli": "^3.0.0",
    "chromatic": "^5.9.0",
    "custom-property-extract": "^1.2.1",
    "deepmerge": "^4.2.2",
    "esbuild": "^0.12.23",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "include-media": "^1.4.10",
    "json-schema-merge-allof": "^0.8.1",
    "json-schema-ref-parser": "^9.0.9",
    "kickstartds": "^0.2.26",
    "lazysizes": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.34.1",
    "storybook-design-token": "^2.2.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "engines": {
    "yarn": "^1.22.10"
  }
}
